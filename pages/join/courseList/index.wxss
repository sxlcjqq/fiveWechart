}
.courselist{
  padding:0 20rpx;
}
.listdiv{
  position: relative;
  font-size: 0;
  line-height: 0;
  padding: 20rpx 0rpx;
  border-bottom:1rpx solid #999999;
}
.listdiv .bg{
  display: block;
  height: 400rpx;
  width: 100%;
}
.listdiv .price{
  right: 0rpx;
  top: 340rpx;
  position: absolute;
  font-size: 34rpx;
  color: #ffffff;
  display: block;
  line-height: 1;
  padding:10rpx 20rpx;
  background: rgba(0,0,0,0.5);
}
.listdiv .name{
  font-size: 34rpx;
  color: #000000;
  line-height: 1.5;
  font-weight: bold;
}
.listdiv .area{
  font-size: 20rpx;
  color: #666666;
  line-height: 1.5;
  border: 1rpx solid #999999;
  padding:5rpx 10rpx;
  border-radius: 5rpx;
  margin-left: 20rpx;
}
.listdiv .star{
width: 45rpx;
height: 45rpx;
display: inline-block;
margin: 10rpx 0 0;
vertical-align: middle;
}
.listdiv .address{
  font-size: 26rpx;
  color: #000000;
  line-height: 1.5;
  margin-top:10rpx;
}